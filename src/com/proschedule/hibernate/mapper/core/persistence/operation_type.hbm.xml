<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.proschedule.core.persistence.model.OperationType" table="ps_operation_type">
    <id column="id" name="id">
      <generator class="sequence">
        <param name="sequence">ps_operation_type_id_seq</param>
      </generator>
    </id>
    <property column="description" length="80" name="description" not-null="true" type="string"/>
  </class>
</hibernate-mapping>
